<div class='container'>
    <div class='row'>
        <div class='col-md-12'>
            <div class='row'>
                <div class='col-md-12'>
                    <div id='admin-body' class='page-body'>
                        <% include user-list %>
                    </div>
                    <div id='user-body' class='page-body'>
                        You are not an Admin<br>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

<script>
    var isAdmin = <%= admin %>;
    if (!isAdmin) {
        var div = document.getElementById('admin-body');
        div.style.display = "none";
    } else {
        var div = document.getElementById('user-body');
        div.style.display = "none";            
    }
</script>